{
  "name": "mycoffeeapp",
  "version": "0.1.0",
  "description": "A Coffee Run App",
  "author": "Aloys Baillet <aloys.baillet@gmail.com>",
  "style": "public/bundle.css",
  "scripts": {
    "build": "browserify -t babelify --presets react jsx/mycoffeeapp.jsx -o public/bundle.js",
    "watch": "watchify -t babelify --presets react jsx/mycoffeeapp.jsx -o public/bundle.js --debug --verbose",
    "test": "jest",
    "rules": "blaze rules.yaml && firebase deploy:rules",
    "deploy": "NODE_ENV=production npm run build && firebase deploy",
    "upload-data": "node data/upload.js"
  },
  "devDependencies": {
    "babel-jest": "^6.0.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "firebase-server": "^0.5.1",
    "firebase-token-generator": "^2.0.0",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^2.1.2",
    "jest-cli": "^0.8.2",
    "mockfirebase": "^0.12.0",
    "proxyquire": "^1.7.3",
    "react-addons-test-utils": "^0.14.3",
    "react-sanfona": "0.0.8",
    "reactify": "^1.1.1",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "firebase": "^2.3.2",
    "firebase-util": "^0.2.5",
    "gulp-notify": "^2.2.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-intl": "^2.0.0-beta-2",
    "react-select": "^1.0.0-beta8",
    "reactfire": "^0.5.1"
  },
  "jest": {
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}

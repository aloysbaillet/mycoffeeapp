{
  "name": "mycoffeeapp",
  "version": "0.1.0",
  "description": "A Coffee Run App",
  "author": "Aloys Baillet <aloys.baillet@gmail.com>",
  "style": "public/bundle.css",
  "scripts": {
    "build": "NODE_ENV=production browserify -t babelify --presets react jsx/mycoffeeapp.jsx -o public/bundle.js",
    "test": "jest",
    "rules": "blaze rules.yaml && firebase deploy:rules",
    "deploy": "npm run build && firebase deploy",
    "upload-data": "node data/upload.js"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "browser-sync": "^2.11.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "eslint-plugin-react": "^3.15.0",
    "firebase-server": "^0.5.1",
    "firebase-token-generator": "^2.0.0",
    "gulp": "gulpjs/gulp.git#4.0",
    "gulp-eslint": "~1.1.1",
    "gulp-header": "~1.7.1",
    "gulp-util": "~3.0.7",
    "jasmine-core": "^2.4.1",
    "jest-cli": "^0.8.2",
    "karma": "^0.13.19",
    "mockfirebase": "^0.12.0",
    "node-sass": "^3.4.2",
    "proxyquire": "^1.7.3",
    "react-addons-test-utils": "^0.14.3",
    "react-sanfona": "0.0.8",
    "react-transform-hmr": "^1.0.1",
    "sass-loader": "^3.1.2",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel": "^6.3.26",
    "classnames": "^2.2.3",
    "firebase": "^2.3.2",
    "firebase-util": "^0.2.5",
    "fireproof": "^3.1.0",
    "gulp-notify": "^2.2.0",
    "history": "^1.17.0",
    "html-webpack-plugin": "^1.7.0",
    "intl": "^1.0.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.6",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "moment": "^2.11.0",
    "node-sass": "^3.4.2",
    "object-assign": "^4.0.1",
    "react": "^0.14.3",
    "react-bootstrap": "^0.28.2",
    "react-check3": "^0.1.1",
    "react-dom": "^0.14.3",
    "react-intl": "^2.0.0-beta-2",
    "react-router": "^1.0.3",
    "react-select": "^1.0.0-beta8",
    "react-timeago": "^2.2.1",
    "reactfire": "^0.5.1",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "underscore": "^1.8.3"
  },
  "jest": {
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
